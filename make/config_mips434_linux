##主机开发环境(ppcgnu_gcc_linux, i386_gcc_linux,mips_gcc_linux)
VERSION       = 434
G_DEVTOOLS	  =	mips_gcc_linux
CROSS_COMPILE = /opt/trendchip/mips-linux-uclibc/usr/bin/mips-linux-
LIB           = -L/opt/trendchip/mips-linux-uclibc/usr/lib
CC			  =	$(CROSS_COMPILE)gcc
AR			  =	$(CROSS_COMPILE)ar crs
G_ENDIAN      = big
INC           = -I/opt/trendchip/mips-linux-uclibc/usr/include